{
    "sysinfo": {
        "interval": 2
    },
    "settings": {
        "page": 3
    },
    "logging": {
        "level": 10
    },
    "constants": {
        "bauds": [
            4800,
            9600,
            19200,
            38400,
            57600,
            115200,
            230400,
            460800
        ],
        "ticks": [
            "CORE",
            "0.1s",
            "0.25s",
            "0.4s",
            "0.6s",
            "0.8s",
            "1s",
            "1.5s",
            "2s"
        ]
    },
    "services": {
        "mqtt": {
            "port": 1883,
            "address": "localhost"
        },
        "serial": {
            "p2-baud": 230400,
            "p2-port": "/dev/ttyAMA1",
            "xb-baud": 460800,
            "xb-port": "/dev/ttyAMA0",
            "head-baud": 115200,
            "head-port": "/dev/ttyUSB0"
        },
        "com": {
            "tick": "1s",
            "topic-batt1": "kevinbot/battery/batt1",
            "topic-batt2": "kevinbot/battery/batt2",
            "topic-sys-uptime": "kevinbot/uptimes/os",
            "topic-core-uptime": "kevinbot/uptimes/core",
            "topic-enabled": "kevinbot/enabled"
        },
        "mpu": {
            "enabled": true,
            "address": 104,
            "update-speed": 0.1,
            "topic-roll": "kevinbot/mpu/roll",
            "topic-pitch": "kevinbot/mpu/pitch",
            "topic-yaw": "kevinbot/mpu/yaw"
        },
        "bme": {
            "update-speed": 0.1,
            "topic-temp": "kevinbot/bme/temperature",
            "topic-humidity": "kevinbot/bme/humidity",
            "topic-pressure": "kevinbot/bme/pressure"
        },
        "data_max": 50
    }
}